<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yxz.youyi">

    <!-- 获取网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DetailsActivity" />
        <activity android:name=".activity.ScreeningActivity" />
        <activity android:name=".activity.AttentionbarActivity" />
        <activity android:name=".activity.PublishpostActivity" />
        <activity android:name=".activity.SubscribeActivity" />
        <activity android:name=".activity.PostdeatilActivity"></activity>
    </application>

</manifest>